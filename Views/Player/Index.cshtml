@model List<Player>
<div>
    <h1>Players</h1>
    <table class="table mt-5">
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Gender</th>
            <th colspan="3"></th>
        </tr>
        @foreach(var player in Model)
        {
            <tr>
                <td>@player.Id</td>
                <td>@player.FirstName</td>
                <td>@player.MiddleName</td>
                <td>@player.LastName</td>
                <td>@player.Gender</td>
                <td><a href="">Edit</a></td>
                <td><a href="">Delete</a></td>
            </tr>
        }
    </table>
</div>